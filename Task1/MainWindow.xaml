<Window x:Class="Task1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task1"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="300">
    <Grid>
        <ListBox x:Name="CheckedListBox" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0, 20, 0, 0">
            <CheckBox Content="30"/>
            <CheckBox Content="31"/>
            <CheckBox Content="32"/>
            <CheckBox Content="33"/>
            <CheckBox Content="34"/>
        </ListBox>
        <Button x:Name="CheckedOutButton" Content="Show me!!!" Height="40" Width="80" Click="CheckedOutButton_Click" VerticalAlignment="Top" Margin="0, 150, 0, 0"/>
    </Grid>
</Window>
