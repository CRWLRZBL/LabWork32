<Window x:Class="Task2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ListBox x:Name="NewListBox" Grid.Row="1"/>
        <ComboBox x:Name="NewComboBox" Grid.Row="2"/>
        <Button x:Name="PutIntoButton" Grid.Row="3" Content="Set info into listbox and combobox" Click="PutIntoButton_Click"/>
        <TextBox x:Name="UserInputTextBox" Grid.Row="0"/>

    </Grid>
</Window>
